{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"rootDir": "./services",
		"outDir": "./dist"
	},
	"include": ["services/**/*"],
	"exclude": ["node_modules", "dist"]
}
